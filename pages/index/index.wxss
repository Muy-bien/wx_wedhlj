@import "../template/Task_template/Task_template.wxss";

.task_list {
  background-color: #fff;
  margin: 10rpx 0;
}

.task_but {
  padding: 20rpx 0;
}

.task_but button {
  width: 127rpx;
  height: 65rpx;
  border-radius: 10rpx;
