.haeder {
  height: 440rpx;
  position: relative;
  padding-top: 68rpx;
  box-sizing: border-box;
  margin-bottom: 17rpx;
}

.haeder_logo{
  width: 200rpx;
  height: 200rpx;
  position: relative;
  margin: 0 auto 12rpx auto;
}

.haeder_logo_bg{
  width: 200rpx;
  height: 200rpx;
}

.haeder_logo_img {
  position: absolute;
  top: 50%; left: 50%;
  transform: translate(-50%,-50%);
  width: 156rpx;
  height: 156rpx;
  border-radius: 100%;
  border: 7rpx solid #fff;
}

.haeder_name{
  display: block;
  color: #fff;
  text-align: center;
  font-size: 28rpx;
}

.haeder_bg{
  width: 100%;
  height: 84rpx;
  position: absolute;
  bottom: 0; left: 0;
}

.Release_task{
  height: 108rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #fff;
}

.Release_task_but{
  width: 229rpx;
  height: 66rpx;
  border-radius: 10rpx;
  line-height: 66rpx;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  box-shadow: 0 0 5rpx 4rpx rgba(239,28,130,0.4);
}

.main{
  padding: 0 50rpx;
}

.main_list{
  height: 99rpx;
  border-bottom: 1px solid #e6e6e6;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 24rpx;
}

.main_list_left{
  display: flex;
  align-items: center;
}

.main_list_left image{
  width: 44rpx;
  height: 47rpx;
  margin-right: 26rpx;
}

.main_list_left text{
  font-size: 28rpx;
  color: #333;
}

.main_list_Arrow{
  width: 10rpx;
  height: 18rpx;
}
